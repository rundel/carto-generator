<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map background-color="#ffffff" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs">


<Style name="world" filter-mode="first">
  <Rule>
    <LineSymbolizer stroke="#cccccc" stroke-width="0.5" />
    <PolygonSymbolizer fill="#eeeeee" />
  </Rule>
</Style>
<Layer
      name="world"
   srs="+proj=latlong +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>world</StyleName>
    <Datasource>
       <Parameter name="file">../data/world_borders</Parameter>
       <Parameter name="type">shape</Parameter>
    </Datasource>
  </Layer>

</Map>
